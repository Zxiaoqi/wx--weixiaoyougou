<tab-view tabs="{{tabs}}"  activeIndex="{{activeIndex}}" bindonChange="changeIndex">
    <view class="order">
        <view class="order-item" wx:for="{{orders}}" wx:key="order_id">
            <view class="order-item-cell">
                <view class="order-title">
                    订单编号
                </view>
                <view class="order-num">
                    {{item.order_number}}
                </view>
            </view>
            <view class="order-item-cell">
                <view class="order-title">
                    订单编号
                </view>
                <view class="order-price">
                    {{item.order_price}}
                </view>
            </view>
            <view class="order-item-cell">
                <view class="order-title">
                    订单编号
                </view>
                <view class="order-date">
                    {{item.formatTime}}
                </view>
            </view>
        </view>
    </view>
</tab-view>
